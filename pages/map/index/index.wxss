.section{
  height: 30px;
  width: 100%;
}
.section input{
  width:90%;
  margin:5px auto;
  border:1px solid #c3c3c3;
  height:30px;
  border-radius: 3px;
  padding: 0 5px;
}
.mapTab{
  margin: 0 0 10rpx 10rpx;
  color:  #666;
  font-size:15px;
  display: flex;
  flex-direction: row;
  justify-content:space-around;
}
.icon-image{
  width:100rpx;
  height:100rpx;
  margin: 20rpx 70rpx 0;
}
.tabText{
  margin:0 0 0 90rpx;
}
.map_container{ 
    height: 600px; 
    border-radius: 10px;
    margin-left: 10rpx;
    margin-right: 10rpx;
    padding: 10rpx 10rpx;
} 
.map{
  width: 100%;
  height: 100%;
}
.map_text{
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0px;
  height: 80px;
  background: #fff;
  padding: 0 15px;
}
text{
  margin: 5px 0;
  display: block;
  font-size:12px;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.h1{
  margin: 15px 0;
  font-size:15px;
}

.map-1{
  width: 140rpx;
  height: 140rpx;
  border-radius: 120rpx;
  background-color: #4D8AD7;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color:#ffffff;
  font-size: 34rpx;
  position: absolute;
  top:0rpx;
  right: 40rpx;
}

.map-1 image{
  width: 50rpx;
  height: 50rpx;
}